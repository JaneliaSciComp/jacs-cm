{
  "rabbit_version": "3.6.12",
  "users": [
    {
      "name": "RABBITMQ_USER",
      "password_hash": "RABBITMQ_PASSWORD",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "JACS-messaging"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "aliveness-test",
      "vhost": "/",
      "durable": false,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "ClientRefresh",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "MouselightErrors",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "UpdatesProcessor",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "IndexingProcessor",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "IndexingRequests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "ModelErrors",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "ModelRefresh",
      "vhost": "/",
      "type": "fanout",
      "durable": false,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "ModelUpdates",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "DocIndexRefresh",
      "vhost": "/",
      "type": "fanout",
      "durable": false,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "DocIndexUpdates",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "ModelErrors",
      "vhost": "/",
      "destination": "MouselightErrors",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    },
    {
      "source": "ModelUpdates",
      "vhost": "/",
      "destination": "UpdatesProcessor",
      "destination_type": "queue",
      "routing_key": "brokerqueue",
      "arguments": {}
    },
    {
      "source": "DocIndexUpdates",
      "vhost": "/",
      "destination": "IndexingProcessor",
      "destination_type": "queue",
      "routing_key": "",
      "arguments": {}
    }
  ]
}
